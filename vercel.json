{
   "version": 2,
   "name": "cashbook-backend",
   "builds": [
      {
         "src": "./server.js",
         "use": "@vercel/node"
      }
   ],
   "routes": [
      {
         "src": "/(.*)",
         "dest": "./index.js",
         "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE",
            "PATCH",
            "OPTIONS"
         ],
         "headers": {
            "Access-Control-Allow-Origin": "*"
         }
      }
   ]
}